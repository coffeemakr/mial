{
  "name": "mial",
  "version": "1.0.0",
  "main": "mial.js",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.5.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && jest --silent false",
