  "keywords": [
    "email",
    "recommendation",
    "levenshtein",
    "domains"
  ],
  "author": "coffeemakr <github@unstable.ch>",
  "repository": {
    "type": "git",
    "url": "https://github.com/coffeemakr/mial.git"
  },
